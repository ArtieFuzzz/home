{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=() camera=() microphone=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' api.fonts.coollabs.io, iconify.design"
        }
      ]
    }
  ]
}
